.content.noticias .row-fluid.fila{
	margin: 15px 180px;
	padding: 10px;
	border: 3px solid;
	border-radius: 5px;
	background-color: #000509;
}

.container .pagination ul > li > a {
	background-color: #1b1b1b;
	border: 1px solid;
}
.container .pagination ul > li.disabled > a,.container .pagination ul > li.disabled > a:hover {
	cursor:default;
	background-color: black;
}
.container .pagination ul > li.active > a {
	background-color: white;
	color: black;
}
